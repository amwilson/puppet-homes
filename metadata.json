{
  "name":         "opentable-homes",
  "version":      "1.0.1",
  "author":       "opentable",
  "license":      "MIT",
  "summary":      "A nodule for managing local users and ssh keys",
  "source":       "https://github.com/opentable/puppet-homes.git",
  "project_page": "https://github.com/opentable/puppet-homes",
  "issues_url":   "https://github.com/opentable/puppet-homes/issues",
  "operatingsystem_support": [
      {
        "operatingsystem": "RedHat",
        "operatingsystemrelease": ["5.0","6.0"]
      },
      { "operatingsystem": "CentOS",
        "operatingsystemrelease": ["5.0","6.0"]
      },
      { "operatingsystem": "Debian",
        "operatingsystemrelease": ["6.0","7.0"]
      },
      { "operatingsystem": "Ubuntu",
        "operatingsystemrelease": ["12.04","14.04"]
      }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.0.0 <5.0.0"
    },
    {
      "name": "opentable/altlib",
      "version_requirement": ">= 0.2.0"
    },
    {
      "name": "herculesteam/augeasproviders",
      "version": ">= 2.0.0 <3.0.0"
    }
  ]
}
